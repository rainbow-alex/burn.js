anon.burn:
	let $foo = ( function() {} )
	print $foo

$ $BURN anon.burn
	* stdout
		<Function>

var.burn:
	let $foo = function() {}
	print $foo

$ $BURN var.burn
	* stdout
		<Function:$foo>

reassigned.burn:
	let $foo = function() {}
	let $bar = $foo
	print $bar

$ $BURN reassigned.burn
	* stdout
		<Function:$foo>

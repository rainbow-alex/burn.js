parameters.burn:
	let $add = function( $a, $b ) {
		return $a + $b
	}
	assert( $add( 1, 2 ) == 3 )

$ $BURN parameters.burn

typed_parameters.burn:
	import test
	import burn.types.Number
	test.assert_throws( function() {
		function( Number $p ) {} ( "string" )
	}, ArgumentError )

$ $BURN typed_parameters.burn

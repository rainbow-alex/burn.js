types.burn:
	import burn.types.Safe
	print 1 is Safe
	print "" is Safe
	import burn
	print burn is Safe

$ $BURN types.burn
	* stdout
		true
		true
		true

safe_fn.burn:
	import burn.types.Safe
	print function() {} is Safe

$ $BURN safe_fn.burn
	* stdout
		true

unsafe_fn.burn:
	import burn.types.Safe
	let $foo
	print function() {
		( $foo )
	} is Safe
	print function() {
		$foo = 3
	} is Safe
	print function() {
		function() {
			( $foo )
		}
	} is Safe

$ $BURN unsafe_fn.burn
	* stdout
		false
		false
		false

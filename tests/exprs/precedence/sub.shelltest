sub_or.burn:
	assert( ( ( 1 - 1 ) or nothing ) == nothing )
	assert( ( 1 - ( 1 or nothing ) ) == 0 )
	assert( ( 1 - 1 or nothing ) == nothing )

$ $BURN --tolerant sub_or.burn

sub_and.burn:
	assert( ( ( 1 - 1 ) and 2 ) == 0 )
	assert( ( 1 - ( 1 and 2 ) ) == -1 )
	assert( ( 1 - 1 and 2 ) == 0 )

$ $BURN --tolerant sub_and.burn


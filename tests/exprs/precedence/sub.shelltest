sub_add.burn:
	assert( ( ( 1 - 1 ) + 1 ) == 1 )
	assert( ( 1 - ( 1 + 1 ) ) == -1 )
	assert( ( 1 - 1 + 1 ) == 1 )

$ $BURN --tolerant sub_add.burn

sub_sub.burn:
	assert( ( ( 1 - 1 ) - 1 ) == -1 )
	assert( ( 1 - ( 1 - 1 ) ) == 1 )
	assert( ( 1 - 1 - 1 ) == -1 )

$ $BURN --tolerant sub_sub.burn

sub_mul.burn:
	assert( ( ( 1 - 1 ) * 2 ) == 0 )
	assert( ( 1 - ( 1 * 2 ) ) == -1 )
	assert( ( 1 - 1 * 2 ) == -1 )

$ $BURN --tolerant sub_mul.burn


add_or.burn:
	try { print ( 0 + false ) or 0 }
	catch TypeError $e { print "TypeError" }
	print 0 + ( false or 0 )
	try { print 0 + false or 0 }
	catch TypeError $e { print "TypeError" }

$ $BURN --tolerant add_or.burn
	* stdout
		TypeError
		0
		TypeError


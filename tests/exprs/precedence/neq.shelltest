// generated by `etc/generate_operator_test.py precedence neq`

neq_or.burn:
	print ( true != true ) or true
	print true != ( true or true )
	print true != true or true

$ $BURN --tolerant neq_or.burn
	* stdout
		true
		false
		true

neq_and.burn:
	print ( true != true ) and false
	print true != ( true and false )
	print true != true and false

$ $BURN --tolerant neq_and.burn
	* stdout
		false
		true
		false

neq_add.burn:
	try { print ( 0 != 0 ) + 0 }
	catch TypeError $e { print "TypeError" }
	print 0 != ( 0 + 0 )
	print 0 != 0 + 0

$ $BURN --tolerant neq_add.burn
	* stdout
		TypeError
		false
		false

neq_sub.burn:
	try { print ( 0 != 0 ) - 0 }
	catch TypeError $e { print "TypeError" }
	print 0 != ( 0 - 0 )
	print 0 != 0 - 0

$ $BURN --tolerant neq_sub.burn
	* stdout
		TypeError
		false
		false

neq_mul.burn:
	try { print ( 0 != 0 ) * 0 }
	catch TypeError $e { print "TypeError" }
	print 0 != ( 0 * 0 )
	print 0 != 0 * 0

$ $BURN --tolerant neq_mul.burn
	* stdout
		TypeError
		false
		false

neq_div.burn:
	try { print ( true != 0 ) / 0 }
	catch TypeError $e { print "TypeError" }
	try { print true != ( 0 / 0 ) }
	catch ValueError $e { print "ValueError" }
	try { print true != 0 / 0 }
	catch ValueError $e { print "ValueError" }

$ $BURN --tolerant neq_div.burn
	* stdout
		TypeError
		ValueError
		ValueError

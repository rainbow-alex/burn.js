gt_or.burn:
	assert( ( ( 1 > 1 ) or nothing ) == nothing )
	assert( ( 1 > ( 1 or nothing ) ) == false )
	assert( ( 1 > 1 or nothing ) == nothing )

$ $BURN --tolerant gt_or.burn

gt_and.burn:
	assert( ( ( 1 > 1 ) and 0 ) == false )
	assert( ( 1 > ( 1 and 0 ) ) == true )
	assert( ( 1 > 1 and 0 ) == false )

$ $BURN --tolerant gt_and.burn

// TODO: gt-not

gt_is.burn:
	nothing > nothing is nothing

$ $BURN --tolerant gt_is.burn
	* exit status nonzero

gt_is_not.burn:
	nothing > nothing is not nothing

$ $BURN --tolerant gt_is_not.burn
	* exit status nonzero

gt_eq.burn:
	nothing > nothing == nothing

$ $BURN --tolerant gt_eq.burn
	* exit status nonzero

gt_neq.burn:
	nothing > nothing != nothing

$ $BURN --tolerant gt_neq.burn
	* exit status nonzero

gt_lt.burn:
	nothing > nothing < nothing

$ $BURN --tolerant gt_lt.burn
	* exit status nonzero

gt_gt.burn:
	nothing > nothing > nothing

$ $BURN --tolerant gt_gt.burn
	* exit status nonzero

gt_lteq.burn:
	nothing > nothing <= nothing

$ $BURN --tolerant gt_lteq.burn
	* exit status nonzero

gt_gteq.burn:
	nothing > nothing >= nothing

$ $BURN --tolerant gt_gteq.burn
	* exit status nonzero


eq_or.burn:
	assert( ( ( false == true ) or nothing ) == nothing )
	assert( ( false == ( true or nothing ) ) == false )
	assert( ( false == true or nothing ) == nothing )

$ $BURN --tolerant eq_or.burn

eq_and.burn:
	assert( ( ( nothing == nothing ) and nothing ) == nothing )
	assert( ( nothing == ( nothing and nothing ) ) == true )
	assert( ( nothing == nothing and nothing ) == nothing )

$ $BURN --tolerant eq_and.burn

// TODO: eq-not

eq_is.burn:
	nothing == nothing is nothing

$ $BURN --tolerant eq_is.burn
	* exit status nonzero

eq_is_not.burn:
	nothing == nothing is not nothing

$ $BURN --tolerant eq_is_not.burn
	* exit status nonzero

eq_eq.burn:
	nothing == nothing == nothing

$ $BURN --tolerant eq_eq.burn
	* exit status nonzero

eq_neq.burn:
	nothing == nothing != nothing

$ $BURN --tolerant eq_neq.burn
	* exit status nonzero

eq_lt.burn:
	nothing == nothing < nothing

$ $BURN --tolerant eq_lt.burn
	* exit status nonzero

eq_gt.burn:
	nothing == nothing > nothing

$ $BURN --tolerant eq_gt.burn
	* exit status nonzero

eq_lteq.burn:
	nothing == nothing <= nothing

$ $BURN --tolerant eq_lteq.burn
	* exit status nonzero

eq_gteq.burn:
	nothing == nothing >= nothing

$ $BURN --tolerant eq_gteq.burn
	* exit status nonzero


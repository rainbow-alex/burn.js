// generated by `etc/generate_operator_test.py precedence eq`

eq_or.burn:
	print ( true == false ) or 0
	try { print true == ( false or 0 ) } catch TypeError $e { print "TypeError" }
	print true == false or 0

$ $BURN --tolerant eq_or.burn
	* stdout
		0
		TypeError
		0

eq_and.burn:
	print ( true == true ) and 0
	try { print true == ( true and 0 ) } catch TypeError $e { print "TypeError" }
	print true == true and 0

$ $BURN --tolerant eq_and.burn
	* stdout
		0
		TypeError
		0

eq_add.burn:
	try { print ( 0 == 0 ) + 0 } catch TypeError $e { print "TypeError" }
	print 0 == ( 0 + 0 )
	print 0 == 0 + 0

$ $BURN --tolerant eq_add.burn
	* stdout
		TypeError
		true
		true

eq_sub.burn:
	try { print ( 0 == 0 ) - 0 } catch TypeError $e { print "TypeError" }
	print 0 == ( 0 - 0 )
	print 0 == 0 - 0

$ $BURN --tolerant eq_sub.burn
	* stdout
		TypeError
		true
		true

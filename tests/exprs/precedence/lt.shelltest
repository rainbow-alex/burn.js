// generated by `etc/generate_operator_test.py precedence lt`

lt_or.burn:
	try { print ( 0 < false ) or 0 }
	catch TypeError $e { print "TypeError" }
	print 0 < ( false or 0 )
	try { print 0 < false or 0 }
	catch TypeError $e { print "TypeError" }

$ $BURN --tolerant lt_or.burn
	* stdout
		TypeError
		false
		TypeError

lt_and.burn:
	try { print ( 0 < true ) and 0 }
	catch TypeError $e { print "TypeError" }
	print 0 < ( true and 0 )
	try { print 0 < true and 0 }
	catch TypeError $e { print "TypeError" }

$ $BURN --tolerant lt_and.burn
	* stdout
		TypeError
		false
		TypeError

lt_add.burn:
	try { print ( 0 < 0 ) + 0 }
	catch TypeError $e { print "TypeError" }
	print 0 < ( 0 + 0 )
	print 0 < 0 + 0

$ $BURN --tolerant lt_add.burn
	* stdout
		TypeError
		false
		false

lt_sub.burn:
	try { print ( 0 < 0 ) - 0 }
	catch TypeError $e { print "TypeError" }
	print 0 < ( 0 - 0 )
	print 0 < 0 - 0

$ $BURN --tolerant lt_sub.burn
	* stdout
		TypeError
		false
		false

lt_mul.burn:
	try { print ( 0 < 0 ) * 0 }
	catch TypeError $e { print "TypeError" }
	print 0 < ( 0 * 0 )
	print 0 < 0 * 0

$ $BURN --tolerant lt_mul.burn
	* stdout
		TypeError
		false
		false

lt_div.burn:
	try { print ( true < 0 ) / 0 }
	catch TypeError $e { print "TypeError" }
	try { print true < ( 0 / 0 ) }
	catch ValueError $e { print "ValueError" }
	try { print true < 0 / 0 }
	catch ValueError $e { print "ValueError" }

$ $BURN --tolerant lt_div.burn
	* stdout
		TypeError
		ValueError
		ValueError

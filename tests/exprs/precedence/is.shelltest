// generated by `etc/generate_operator_test.py precedence is`

is_or.burn:
	try { print ( true is false ) or Something }
	catch TypeError $e { print "TypeError" }
	print true is ( false or Something )
	try { print true is false or Something }
	catch TypeError $e { print "TypeError" }

$ $BURN --tolerant is_or.burn
	* stdout
		TypeError
		true
		TypeError

is_and.burn:
	try { print ( true is true ) and Something }
	catch TypeError $e { print "TypeError" }
	print true is ( true and Something )
	try { print true is true and Something }
	catch TypeError $e { print "TypeError" }

$ $BURN --tolerant is_and.burn
	* stdout
		TypeError
		true
		TypeError

is_div.burn:
	try { print ( true is 0 ) / 0 }
	catch TypeError $e { print "TypeError" }
	try { print true is ( 0 / 0 ) }
	catch ValueError $e { print "ValueError" }
	try { print true is 0 / 0 }
	catch ValueError $e { print "ValueError" }

$ $BURN --tolerant is_div.burn
	* stdout
		TypeError
		ValueError
		ValueError

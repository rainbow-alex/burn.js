eq.burn:
	print 1 == 1
	print 1 == 2

$ burn eq.burn
	* stdout
		true
		false

int_eq_string.burn:
	try {
		3 == "3"
	} catch TypeError $e {
		print "TypeError"
	}

$ burn int_eq_string.burn
	* stdout
		TypeError

int_eq_float.burn:
	try {
		3 == 3.0
	} catch TypeError $e {
		print "TypeError"
	}

$ burn int_eq_string.burn
	* stdout
		TypeError


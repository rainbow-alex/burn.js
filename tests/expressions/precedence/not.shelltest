not_or.burn:
	assert( ( ( not nothing ) or true ) == true )
	assert( ( not ( nothing or true ) ) == false )
	assert( ( not nothing or true ) == true )

$ $BURN not_or.burn

not_and.burn:
	assert( ( ( not nothing ) and nothing ) == nothing )
	assert( ( not ( nothing and nothing ) ) == true )
	assert( ( not nothing and nothing ) == nothing )

$ $BURN not_and.burn

not_not.burn:
	not not true
$ $BURN not_not.burn
	* exit status nonzero

not_is.burn:
	assert( ( ( not true ) is Something ) == true )
	assert( ( not ( true is Something ) ) == false )
	assert( ( not true is Something ) == false )

$ $BURN not_is.burn

not_is_not.burn:
	assert( ( ( not true ) is not Nothing ) == true )
	assert( ( not ( true is not Nothing ) ) == false )
	assert( ( not true is not Nothing ) == false )

$ $BURN not_is_not.burn

not_eq.burn:
	import test
	test.assert_throws( function() { ( not 1 ) == 1 }, TypeError )
	assert( ( not ( 1 == 1 ) ) == false )
	assert( ( not 1 == 1 ) == false )

$ $BURN not_eq.burn

not_neq.burn:
	import test
	test.assert_throws( function() { ( not 1 ) != 1 }, TypeError )
	assert( ( not ( 1 != 1 ) ) != false )
	assert( ( not 1 != 1 ) != false )

$ $BURN not_neq.burn

not_lt.burn:
	import test
	test.assert_throws( function() { ( not 1 ) < 1 }, TypeError )
	assert( ( not ( 1 < 1 ) ) != false )
	assert( ( not 1 < 1 ) != false )

$ $BURN not_lt.burn

not_gt.burn:
	import test
	test.assert_throws( function() { ( not 1 ) > 1 }, TypeError )
	assert( ( not ( 1 > 1 ) ) != false )
	assert( ( not 1 > 1 ) != false )

$ $BURN not_gt.burn

not_lteq.burn:
	import test
	test.assert_throws( function() { ( not 1 ) <= 1 }, TypeError )
	assert( ( not ( 1 <= 1 ) ) == false )
	assert( ( not 1 <= 1 ) == false )

$ $BURN not_lteq.burn

not_eq.burn:
	import test
	test.assert_throws( function() { ( not 1 ) >= 1 }, TypeError )
	assert( ( not ( 1 >= 1 ) ) == false )
	assert( ( not 1 >= 1 ) == false )

$ $BURN not_eq.burn


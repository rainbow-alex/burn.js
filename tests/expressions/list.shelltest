empty.burn:
	let $list = []
	print $list.length

$ $BURN empty.burn
	* stdout
		0

list.burn:
	let $list = [ 1, 2, 3 ]
	print $list.length

$ $BURN list.burn
	* stdout
		3

trailing_comma.burn:
	let $list = [ 1, 2, 3, ]
	print $list.length

$ $BURN trailing_comma.burn
	* stdout
		3

newlines.burn:
	let $list = [
		"foo",
		"bar",
		"baz",
	]
	print $list.length

$ $BURN newlines.burn
	* stdout
		3

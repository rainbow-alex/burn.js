test1.burn:
	print ( true or true ) and false
	print true or ( true and false )
	print true or true and false

$ burn --tolerant test1.burn
	* stdout
		false
		true
		true

test2.burn:
	print ( false and true ) or true
	print false and ( true or true )
	print false and true or true

$ burn --tolerant test2.burn
	* stdout
		true
		false
		true

test3.burn:
	print ( not true ) or true
	print not ( true or true )
	print not true or true

$ burn --tolerant test3.burn
	* stdout
		true
		false
		true

test4.burn:
	print ( not true ) and false
	print not ( true and false )
	print not true and false

$ burn --tolerant test4.burn
	* stdout
		false
		true
		false

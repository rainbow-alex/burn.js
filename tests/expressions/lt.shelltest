lt.burn:
	print 3 < 4

$ burn lt.burn
	* stdout
		true

int_lt_float.burn:
	print 3 < 3.5
	print 3.5 < 4

$ burn int_lt_float.burn
	* stdout
		true
		true

strings.burn:
	print "a" < "b"

$ burn strings.burn
	* stdout
		true

int_lt_string.burn:
	try {
		3 < "4"
	} catch TypeError $e {
		print "TypeError"
	}

$ burn int_lt_string.burn
	* stdout
		TypeError

err.burn:
	< 3
$ burn err.burn
	* exit status nonzero
err.burn:
	3 <
$ burn err.burn
	* exit status nonzero
err.burn:
	<
$ burn err.burn
	* exit status nonzero
err.burn:
	1 < 2 < 3
$ burn err.burn
	* exit status nonzero

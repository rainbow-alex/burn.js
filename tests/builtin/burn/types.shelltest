types_primitive.burn:
	import burn.types
	print nothing is Nothing
	print true is Boolean
	print 1 is Integer
	print 1.2 is Float
	print "" is String
	print function() {} is Function
	print types is Module
	print Type is Type

$ $BURN types_primitive.burn
	* stdout
		true
		true
		true
		true
		true
		true
		true
		true

types_lang.burn:
	import burn.types
	print 1 is types.Safe
	print Type is types.Permanent
	print 1 is types.Truthy

$ $BURN types_lang.burn
	* stdout
		true
		true
		true

derivative.burn:
	import burn.types
	print nothing is not Something
	print 1 is Something
	print "" is types.Falsy
	print 2 is types.Number and 1.2 is types.Number

$ $BURN derivative.burn
	* stdout
		true
		true
		true
		true

